{
  "name": "next-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "npx rimraf .next out",
    "dev": "npx next dev",
    "build": "npx next build",
    "next": "npm run clean && npm run build && npx next start",
    "export": "npm run clean && npm run build && npx next export",
    "prenexpress": "npx rimraf .nexpress && npx tsc -p nexpress/tsconfig.json",
    "dev:nexpress": "npm run prenexpress && npx cross-env NODE_ENV=\"development\" node .nexpress/main.js",
    "nexpress": "npm run build && node .nexpress/main.js",
    "lint": "npx next lint"
  },
  "dependencies": {
    "@bizhermit/basic-utils": "^2.0.0",
    "@bizhermit/time": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cookies-next": "^2.1.1",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0"
  },
  "devDependencies": {
    "@styled-jsx/plugin-sass": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "typescript": "^4.9.3"
  }
}
